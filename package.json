{
  "name": "ineedthis",
  "version": "0.0.7",
  "description": "Stateful dependency management",
  "main": "./lib/index.js",
  "types": "./lib.es6/index.d.ts",
  "scripts": {
    "build:babel": "babel --source-maps=inline lib.es6/ --out-dir lib/",
    "build": "npm run clean && tsc && npm run build:babel",
    "build:watch": "concurrently 'tsc -w' 'npm run build:babel -- -w'",
    "test": "npm run build && npm run test:nobuild",
    "test:nobuild": "mocha $MOCHA_OPTS",
    "test:watch": "nodemon --watch lib --watch test -x 'npm -s run test:nobuild'",
    "lint": "tslint 'src/**/*.ts'",
    "lint:watch": "nodemon --watch src -x 'npm -s run lint'",
    "dev": "concurrently --prefix '[{name}]' --names 'TS,Babel,Mocha,Lint' 'tsc -w' 'npm run build:babel -- -w' 'npm run test:watch' 'npm run lint:watch'",
    "clean": "rimraf lib lib.es6",
    "prepublish": "npm run build"
  },
  "files": [
    "lib",
    "lib.es6/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpittarelli/ineedthis.git"
  },
  "keywords": [
    "service",
    "component",
    "module",
    "state",
    "dependency"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "George Pittarelli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gpittarelli/ineedthis/issues"
  },
  "homepage": "https://github.com/gpittarelli/ineedthis#readme",
  "devDependencies": {
    "@types/node": "7.0.5",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "concurrently": "3.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.11",
    "tslint": "4.5.1",
    "typescript": "2.1.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.22.0"
  }
}
